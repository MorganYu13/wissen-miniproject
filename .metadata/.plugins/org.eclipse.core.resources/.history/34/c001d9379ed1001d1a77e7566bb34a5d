package com.wissen.doctorspatientconsolidateservice.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;


import com.wissen.doctorspatientconsolidateservice.dao.AppointmentFullDao;
import com.wissen.doctorspatientconsolidateservice.model.AppointmentAggregate;
import com.wissen.doctorspatientconsolidateservice.model.AppointmentFull;
import com.wissen.doctorspatientconsolidateservice.service.AppointmentFullService;

@Service
public class AppointmentFullServiceImpl implements AppointmentFullService {

	@Autowired
	private AppointmentFullDao appointmentFullDao; 
	
	
	@Override
	public List<AppointmentAggregate> fetchAllAppointmentFull() {
		return appointmentFullDao.fetchAllAppointmentFull(); 
	}
	
	@Override
	public AppointmentAggregate fetchAppointmentFull(String id) {
		return appointmentFullDao.fetchAppointmentFull(id); 
	}

}
